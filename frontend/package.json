{
  "name": "@md-wiki/frontend",
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "NODE_ENV=development sst-env -- webpack serve",
    "build": "sst-env -- webpack",
    "test": "jest",
    "lint": "npx eslint \"src/**/*.@(tsx|ts|jsx|js)\" --fix ; npx prettier --check \"src/**/*.@(tsx|ts|jsx|js)\" --write"
  },
  "devDependencies": {
    "@serverless-stack/static-site-env": "^1.15.16",
    "@types/jest": "^27.0.2",
    "@types/react-modal": "^3.13.1",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "css-loader": "^6.7.1",
    "dts-css-modules-loader": "^2.0.1",
    "esbuild": "^0.13.15",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^2.16.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "fork-ts-checker-webpack-plugin": "^6.4.2",
