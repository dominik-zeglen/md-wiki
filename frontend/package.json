    "@tanstack/react-query": "^4.13.5",
    "@types/lodash": "^4.14.177",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "axios": "^1.1.3",
    "clsx": "^1.1.1",
    "color": "^4.0.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-click-away-listener": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.38.0",
    "react-inlinesvg": "^3.0.0",
    "react-markdown": "^8.0.3",
